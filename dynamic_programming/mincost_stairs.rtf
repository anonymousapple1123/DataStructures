Reach the desired stair with minimum cost.

function climbMin(cost){
 const n = cost.length;
 cost minCost = [];

 minCost[0] = cost[0];
 minCost[1] = cost[1];

 for(let i = 2; i< n;i++){
 minCost[i] = Math.min(minCost[i-1], minCost[i-2]) + cost[i];
 }
 }
